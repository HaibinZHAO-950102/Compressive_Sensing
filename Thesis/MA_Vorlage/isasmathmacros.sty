% Abbreviation for \displaystyle
\newcommand{\ds}{\displaystyle}

% Abbreviation for \textstyle
\newcommand{\ns}{\textstyle}

% Real numbers
\newcommand{\IR}{\ensuremath{\mathds{R}}}

% Natural numbers
\newcommand{\IN}{\ensuremath{\mathds{N}}}

% Integers
\newcommand{\IZ}{\ensuremath{\mathds{Z}}}

% Complex numbers
\newcommand{\IC}{\ensuremath{\mathds{C}}}

% IR^{#1 x #1}
\newcommand{\IRnm}[2]{\ensuremath{\IR^{{#1}\times{#2}}}}

% Differential of a function
\newcommand{\dd}{\operatorname{d}\!}

% Differentiation
\newcommand{\diff}[2]{\frac{\dd{#1}}{\dd{#2}}}

% Partial differentiation			
\newcommand{\pardiff}[2]{\frac{\partial{#1}}{\partial{#2}}}

% n-dimensional integral from -inft to inft
\newcommand{\intndim}{\ds\int_{-\infty}^\infty \dots \int_{-\infty}^\infty}

% Round up
\newcommand{\ceil}[1]{\ensuremath{\lceil{#1}\rceil}}

% Round down
\newcommand{\floor}[1]{\ensuremath{\lfloor{#1}\rfloor}}

% Inverse
\newcommand{\inv}[1]{\left(#1\right)^{-1}}

% arg min operator
\DeclareMathOperator*{\argmin}{arg\,min}

% arg max operator
\DeclareMathOperator*{\argmax}{arg\,max}

% diag operator
\DeclareMathOperator{\diag}{diag}

% Probability operator
\renewcommand{\Pr}{\operatorname{P}}

% Random variable
\newcommand{\rv}[1]{\ensuremath{{\boldsymbol{#1}}}}

% Vector
\renewcommand{\vec}[1]{\ensuremath{{\underline{#1}}}}

% Random vector
\newcommand{\rvec}[1]{\ensuremath{{\boldsymbol{\underline{#1}}}}}

% Matrix
\newcommand{\mat}[1]{{\ensuremath{{\mathbf{#1}}}}}

% Transpose
\newcommand{\T}{^\mathrm{T}}

% Realization of a random variable / vector
\newcommand{\real}[1]{\ensuremath{\accentset{\star}{#1}}}

% Mean of a random variable / vector
\newcommand{\mean}[1]{\ensuremath{\hat{#1}}}

% Covariance matrix
\newcommand{\C}{\ensuremath{{\mC}}}

% Expectation
\newcommand{\E}[2][]{\ensuremath{{\operatorfont{E}}_{#1}\!\left\{#2\right\}}}

% Variance
\newcommand{\Var}[2][]{\ensuremath{{\operatorfont{Var}}\!\left\{\rv{#1}\right\}}}

% Covariance
\newcommand{\Cov}[2][]{\ensuremath{{\operatorfont{Cov}}_{#1}\!\left\{#2\right\}}}

% Normal distribution
\newcommand{\Gaussian}{\ensuremath{\mathcal{N}}}

% Uniform distribution
\newcommand{\Uniform}{\ensuremath{\mathcal{U}}}

